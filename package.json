{
  "name": "t-shirt-generator",
  "version": "1.0.0",
  "description": "Slack-integrated t-shirt graphic generator using Amazon Bedrock",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' 'infrastructure/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'infrastructure/**/*.ts' 'test/**/*.ts' --fix",
    "type-check": "tsc --noEmit",
    "cdk": "cdk",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "cdk:deploy": "cdk deploy --all",
    "cdk:destroy": "cdk destroy --all"
  },
  "keywords": [
    "slack",
    "bedrock",
    "image-generation",
    "t-shirt",
    "aws",
    "cdk"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.159",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "aws-cdk": "^2.1100.1",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-lambda-powertools/logger": "^2.30.0",
    "@aws-lambda-powertools/parameters": "^2.30.0",
    "@aws-sdk/client-bedrock-runtime": "^3.958.0",
    "@aws-sdk/client-dynamodb": "^3.958.0",
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/client-secrets-manager": "^3.958.0",
    "@aws-sdk/client-sqs": "^3.960.0",
    "@aws-sdk/lib-dynamodb": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@google/genai": "^1.34.0",
    "@google/generative-ai": "^0.24.1",
    "@slack/web-api": "^7.13.0",
    "aws-cdk-lib": "^2.233.0",
    "constructs": "^10.4.4",
    "sharp": "^0.34.5",
    "uuid": "^13.0.0",
    "zod": "^4.3.3"
  },
  "directories": {
    "test": "test"
  },
  "type": "commonjs"
}
